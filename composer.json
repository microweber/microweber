{
    "name": "microweber/microweber",
    "type": "application",
    "description": "New generation CMS with drag and drop",
    "keywords": [
        "cms",
        "drag and drop",
        "shop",
        "blog",
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.microweberapi.com/packages.json"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "laravel/framework": "9.1.*",
        "laravel/passport": "*",
        "laravel/sanctum": "*",
        "laravel/socialite": "*",
        "laravel/dusk": "*",
        "arcanedev/seo-helper": "*",
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-dompdf": "*",
        "composer/composer": "*",
        "composer/installers": "1.9.0",
        "darkaonline/l5-swagger": "*",
        "doctrine/dbal": "3.3.3",
        "enshrined/svg-sanitize": "0.15.0",
        "erusev/parsedown": "1.7.4",
        "ezyang/htmlpurifier": "*",
        "finlet/flexmail": "dev-master",
        "florianv/exchanger": "2.x-dev",
        "florianv/swap": "^4.3",
        "fruitcake/laravel-cors": "*",
        "geoip2/geoip2": "~2.0",
        "graham-campbell/markdown": "*",
        "graham-campbell/security-core": "*",
        "guzzlehttp/guzzle": "*",
        "jenssegers/agent": "^3.0@dev",
        "jeremeamia/superclosure": "*",
        "knplabs/knp-menu": "^3.2@dev",

        "laravelcollective/html": "*",
        "lcobucci/jwt": "*",
        "league/csv": "^9.0@dev",
        "league/omnipay": "*",
        "livewire/livewire": "*",
        "mailerlite/mailerlite-api-v2-php-sdk": "*",
        "mtrajano/laravel-swagger": "0.6.4",
        "omnipay/authorizenet": "*",
        "omnipay/mollie": "*",
        "omnipay/paypal": "*",
        "omnipay/stripe": "*",
        "opis/closure": "^3.6@dev",
        "phpdocumentor/reflection-docblock": "^5.0@dev",
        "phpoffice/phpspreadsheet": "1.16.0",
        "predis/predis": "2.0.x-dev",
        "rtconner/laravel-tagging": "*",
        "scssphp/scssphp": "1.10.0",
        "simshaun/recurr": "*",
        "spatie/laravel-permission": "*",
        "symfony/intl": "*",
        "symfony/translation": "*",
        "ticketswap/omnipay-przelewy24": "^2.0",
        "tightenco/ziggy": "*",
        "torann/laravel-repository": "*",
        "tucker-eric/eloquentfilter": "*",
        "twig/twig": "*",
        "whitecube/lingua": "1.1.0",
        "wikimedia/composer-merge-plugin": "dev-master",
        "wikimedia/less.php": "3.1.0",

        "microweber-packages/microweber-class-alias": "dev-master",
        "microweber-templates/new-world": "5.1",
        "microweber-templates/shopmag": "3.6",
        "microweber-templates/bootstrap5": "1.2",
        "microweber-packages/composer-client": "1.9",
        "microweber-modules/standalone-updater": "3.5"
    },
    "require-dev": {
        "laravel/sail": "*",
        "fzaninotto/faker": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "*",
        "phpunit/phpunit": "9.5.19",
        "laravel/tinker": "*",
        "orchestra/testbench": "*",
        "spatie/laravel-ignition": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Microweber\\": "src/Microweber/",
            "MicroweberPackages\\": "src/MicroweberPackages/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "github-protocols": [
            "https",
            "http",
            "git"
        ],
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "homepage": "http://microweber.com",
    "license": "MIT",
    "support": {
        "email": "support@microweber.com",
        "irc": "irc://irc.freenode.org/microweber"
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "src/MicroweberPackages/*/composer.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "laravel": {
            "dont-discover": [
                "laravel/passport",
                "laravel/sanctum",
                "livewire/livewire",
                "spatie/laravel-ignition",
                "barryvdh/laravel-debugbar"
            ]
        }
    }
}
