{
    "name": "microweber-packages/frontend-assets",
    "description": "Microweber CMS - Frontend Assets",
    "keywords": [
        "microweber-packages",
        "laravel",
        "microweber",
        "microweber-frontend-assets"
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MicroweberPackages\\FrontendAssets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroweberPackages\\FrontendAssets\\Tests\\": "tests/"
        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MicroweberPackages\\FrontendAssets\\MicroweberFrontendAssetsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
