{
  "name": "microweber-packages/microweber-template-manager",
  "description": "Microweber template manager.",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Microweber",
      "email": "info@microweber.com"
    },
    {
      "name": "Bozhidar Slaveykov",
      "email": "bobi@microweber.com"
    }
  ],
  "require": {
    "microweber-packages/microweber-helpers": "dev-master"
  },

  "autoload": {
    "psr-4": {
      "MicroweberPackages\\Utils\\TemplateManager\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MicroweberPackages\\Utils\\TemplateManager\\TemplateManagerServiceProvider"
      ],
      "aliases": {
        "OptionManager": "MicroweberPackages\\Utils\\TemplateManager\\TemplateManagerFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "homepage": "http://microweber.com",
  "license": "MIT",
  "support": {
    "email": "support@microweber.com"
  }
}
