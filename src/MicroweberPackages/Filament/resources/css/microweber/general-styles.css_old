
.x-close-modal-link {
    @apply absolute top-3 right-3 text-black dark:text-white cursor-pointer text-2xl;
}

.modal-header, .modal-body, .modal-footer {
    @apply text-black dark:text-white border-0;
}

.font-weight-bold {
    @apply font-bold;
}

.form-label {
    @apply text-black dark:text-white;
}

.live-edit-label {
    padding: 0 7px;

    text-rendering: optimizelegibility;
    -webkit-font-smoothing: antialiased;
    font-size: 9.75px;
    letter-spacing: 0.75px;
    line-height: 16px;
    text-overflow: ellipsis;

    @apply font-semibold text-gray-600 dark:text-white ps-0 block overflow-hidden box-border uppercase my-2;
}

.form-check-input[type="checkbox"] {
    @apply w-4 h-4 text-blue-600  border-gray-300 rounded focus:ring-blue-500 dark:focus:ring-blue-600 dark:ring-offset-gray-800 focus:ring-2 dark:bg-gray-700 dark:border-gray-600;
}

.form-check {
    @apply flex items-center mb-4 !gap-2;
}


.mw-live-edit-resolutions-wrapper, .mw-live-edit-right-sidebar-wrapper {
    @apply !bg-gray-100 dark:!bg-black dark:text-white rounded p-0.5 max-h-9 flex items-center justify-center gap-2;
}

.mw-live-edit-resolutions-wrapper .live-edit-toolbar-buttons, .mw-live-edit-right-sidebar-wrapper .live-edit-toolbar-buttons {
    @apply rounded !p-1.5 border-2 border-gray-300 bg-gray-100 dark:text-white dark:bg-gray-900  text-gray-600 dark:text-white shadow-none border-none;
}

.mw-live-edit-resolutions-wrapper .live-edit-toolbar-buttons:hover, .mw-live-edit-right-sidebar-wrapper .live-edit-toolbar-buttons:hover, .mw-live-edit-resolutions-wrapper .live-edit-toolbar-buttons:focus, .mw-live-edit-right-sidebar-wrapper .live-edit-toolbar-buttons:focus, .mw-live-edit-resolutions-wrapper .live-edit-toolbar-buttons:active, .mw-live-edit-right-sidebar-wrapper .live-edit-toolbar-buttons:active, .mw-live-edit-resolutions-wrapper .live-edit-toolbar-buttons.active, .mw-live-edit-right-sidebar-wrapper .live-edit-toolbar-buttons.active {
    @apply !bg-white dark:!bg-gray-950 dark:text-white border-none;
}


.mw-live-edit-resolutions-wrapper svg, .mw-live-edit-right-sidebar-wrapper svg {
    @apply fill-black;
}


.admin-thumb-item-uploader,
input.mw-uploader-input,
.mw-filepicker-root .mw-ui-field-holder input.mw-uploader-input {
    @apply absolute top-0 left-0 right-0 bottom-0 !w-full !h-full opacity-0 cursor-pointer;
    font-size: 99px;

}

.cat_selector_view_ctrl {
    clear: both;
    padding: 7px 10px;
    font-size: 11px;
    position: absolute;
    z-index: 2;
    right: 15px;
    top: 6px;
    display: none;
}

.cat_selector_view_ctrl a {
    padding: 2px 12px;
    border: 1px solid transparent;
    font-size: 11px;
    border-radius: 2px;
}

.cat_selector_view_ctrl a.active {
    background: #E1E1E1;
    border-color: #BDBBBB;
}


.mw-tag-selector.mw-ui-field-full {
    @apply w-full;
}

.mw-tag-selector > span > i:after {
    font-family: Microweber;
    font-style: normal;
}

.mw-tag-selector > span > i.have_category:after {
    content: '\0071'
}

.mw-tag-selector > span > i.is_page:after {
    content: '\0071';
}

.mw-tag-selector > span > i.category_element:after {
    content: '\0063';
}

.mw-tag-selector > span > i.is_shop:after {
    content: 's';
}

.mw-tag-selector > span > i.is_blog:after {
    content: '\0062';
}

.mw-tag-selector > span > i.is_home:after {
    content: '\0105'
}

div.mw-custom-fields-tags a.ui-draggable-dragging:after {
    display: none !important;
}

.mw-tag-selector .mw-ui-invisible-field {
    font-size: 14px;
    padding: 7px 0;
}


