.mw-file-drop-zone:hover {
    border-color: rgba(0, 0, 0, 0.125);
    background-color: rgba(0, 0, 0, 0.1);
}

.mw-file-drop-zone .mw-file-drop-zone-holder {
    padding: 40px;
    width: 100%;
    display: flex;
    flex-flow: column;
    align-items: center;
    justify-content: space-between;
}

.mw-file-drop-zone .mw-file-drop-zone-holder .mw-file-drop-zone-img {
    background: url('data:image/svg+xml,<svg fill="currentColor" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 10.4 7.6" style="enable-background:new 0 0 10.4 7.6;" xml:space="preserve"><style type="text/css">.st0{fill:%23EBEBEB;}</style><path class="st0" d="M9.4,0.2H1.1c-0.5,0-0.9,0.4-0.9,0.9v5.4c0,0.5,0.4,0.9,0.9,0.9h8.3c0.5,0,0.9-0.4,0.9-0.9V1.1C10.3,0.6,9.9,0.2,9.4,0.2 M9.8,6.9H0.6V0.8h9.2V6.9 M6.8,2.7l-2.3,3L3,3.7l-2.3,3h9L6.8,2.7z"/></svg>');
    width: 70px;
    height: 55px;
    background-size: contain;
    margin-bottom: 20px;
    background-repeat: no-repeat;
    background-position: center center;
}

.mw-file-drop-zone .mw-file-drop-zone-holder .mw-ui-progress-small {
    width: 180px;
    @apply mb-5 max-w-full;
}

.mw-file-drop-zone .mw-file-drop-zone-holder .mw-ui-btn {

    @apply mb-5;
}

.mw-file-drop-zone .mw-file-drop-zone-holder p {
    @apply text-gray-600 dark:text-white mb-0;
}

.mw-file-drop-zone.mw-file-drop-zone-small {
    background-image: unset;
    /*width: auto;*/
    display: inline-block;
}

.mw-file-drop-zone.mw-file-drop-zone-small .mw-file-drop-zone-holder {

    @apply text-center p-2;
}

.mw-file-drop-zone.mw-file-drop-zone-small .mw-file-drop-zone-holder .mw-ui-btn {
    @apply mb-0;
}

.mw-file-drop-zone.mw-file-drop-square-zone.mw-file-drop-small-zone {
    min-width: 115px;
}

.mw-file-drop-zone.mw-file-drop-square-zone:after {
    content: "";
    display: block;
    padding-bottom: 100%;
}

.mw-file-drop-zone.mw-file-drop-square-zone .mw-file-drop-zone-holder {
    @apply justify-center absolute left-0 top-0 bottom-0 right-0;

}

.mw-uploader-type-holder, .mw-ui-field-holder {
    @apply relative w-full border-0;

}


.mw-ui-progress {
    position: relative;
    height: 19px;
    min-width: 50px;
    overflow: hidden;
    white-space: nowrap;
    background-color: #EEEEEE;
    border: 1px solid #919191;
    border-radius: 10px;
    display: flex;
    align-items: center; }

.mw-ui-progress .mw-ui-progress-bar {
    max-width: 100%;
    height: 100%;
    background-color: #4592ff;
    position: absolute;
    top: 0;
    left: 0; }

.mw-ui-progress-info {
    color: rgba(255, 255, 255, 0.8);
    font-size: 9px;
    position: relative;
    display: inline-block;
    max-width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding: 0 0 0 6px;
    z-index: 2; }

.mw-ui-progress-percent {
    font-weight: bold;
    color: white;
    margin-inline-start: 5px;
    font-size: 9px;
    position: relative;
    display: inline-block;
    z-index: 2; }

/* Small progressbar */
.mw-ui-progress-small,
.mw-ui-progress-small .mw-ui-progress-bar {
    height: 8px;
    overflow: hidden;
    position: relative;
    max-width: 100%; }

.mw-ui-progress-small .mw-ui-progress-bar {
    height: 100%; }

.mw-ui-progress-small {
    background-color: #e9ecef;
    border: 1px solid #919191;
    border-radius: 10px; }

.mw-ui-progress-small .mw-ui-progress-bar {
    background-color: #4592ff; }

.mw-ui-progress-small .mw-ui-progress-info,
.mw-ui-progress-small .mw-ui-progress-percent {
    @apply hidden; }

.mw-file-drop-zone .mw-file-drop-zone-holder p {
    @apply text-gray-600 dark:text-white mb-0;
}


.form-control {
    @apply py-1 px-2 min-h-10;

}

.dropdown-item {
    @apply py-1 px-3 mb-3;
}

.mw-file-manager-search-button {
    @apply flex items-center justify-center h-11 w-11;
    border: 1px solid #ececec;

}

.settings-title-inside {
    @apply text-xl font-bold text-gray-900 dark:text-white dark:text-white leading-5;
}

.mw-dialog-skin-default .mw-dialog-header {
    @apply !p-5;
}

.mw-file-manager-list-item-thumb {
    @apply !bg-contain;
}


.mw-dialog-container {
    @apply bg-white dark:bg-gray-900 dark:text-white !p-6;
}

.mw-dialog-header {
    @apply bg-white dark:!bg-gray-950 dark:text-white;
}

.mw-dialog-footer {
    @apply bg-white dark:bg-gray-900 dark:text-white !p-6 !border-0 flex items-center justify-end gap-3 !m-0;
}

.mw-dialog-footer .btn {
    @apply mb-0;
}

.admin-toolbar-buttons {
    transition: 0.25s ease;
}

.admin-toolbar-buttons:hover {
    transform: scale(1.1);
}

.mw-icon-selector-form-control-range{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 1px;
    z-index: 7;
    background: #d3d3d3;
    --thumb-size: 12px;
    accent-color: #0078ff;
    outline: none;
}




.mw-icon-selector-form-control-range::-webkit-slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    width: var(--thumb-size);
    height:  var(--thumb-size);
    background: #0078ff;
    cursor: pointer;
    border-radius:50px;
}

.mw-icon-selector-form-control-range::-moz-range-thumb {
    width:  var(--thumb-size);
    height:  var(--thumb-size);
    background: #0078ff;
    cursor: pointer;
    border-radius:50px;
}


.input-group {
    position: relative;
    display: flex;
    flex-wrap: wrap;
    align-items: stretch;
    width: 100%;
}

.mwiconlist-settings-section-block-item .form-control {
    @apply border-0;
    width: 200px;
}

.mwiconlist-settings-section-block-item.input-group.input-group-flat {
    @apply shadow;
}

.fi-sidebar-group-items {
    @apply gap-y-4;
}


.mw-post-media-img {
    @apply block bg-no-repeat bg-center bg-cover cursor-grab bg-[#f2f3f5] overflow-hidden absolute top-0 left-0 w-full h-full;
}

.admin-thumbs-holder {
    @apply pt-0 inline-block align-top w-full;
}

.admin-thumb-item {
    --admin-thumb-item-size: 110px;
    --admin-thumb-item-gap: 8px;
    @apply block relative float-left  my-0 border border-[#cfcfcf] cursor-grab;
    width: var(--admin-thumb-item-size);
    height: var(--admin-thumb-item-size);
    margin: calc(var(--admin-thumb-item-gap) / 2);
}


.admin-thumbs-holder .admin-thumb-item:first-child {
    width: calc((var(--admin-thumb-item-size) * 2) + var(--admin-thumb-item-gap));
    height: calc((var(--admin-thumb-item-size) * 2) + var(--admin-thumb-item-gap));
 }

.admin-thumb-item:after {
    @apply absolute block content-[''] w-full h-full top-0 left-0;
}

.admin-thumb-item .form-check {
    @apply transition-all duration-300 absolute left-0 top-0;
}

.admin-thumbs-holder .admin-thumb-item.mw-post-media-img {
    @apply inline-block w-full h-full;
}

.mw-post-media-img--header{
    @apply absolute top-[0px] left-[0px] flex justify-end items-center w-full relative;
}

.image-settings {
    @apply fill-white text-[20px] text-white w-[20px] h-[20px] !cursor-pointer  transition-all duration-300 opacity-70 border-0 z-10;
}

.admin-thumb-item:hover .image-settings {
    @apply opacity-100;
}

.admin-thumb-item:hover .image-settings.remove-image {
    @apply fill-[#ff4f52];
}



.mw-tree-item-content-root {
    @apply ms-3;
}


.mw-tags--container {
    @apply flex flex-wrap gap-2;
}

.mw-control-box {
    @apply !p-3;
}
